<html>
<head>
    <script type="text/javascript" src="js/jQuery.js"></script>
    <link rel="stylesheet" href="css/skeleton.css">
    <title>Crossword Solver</title>
</head>
<body>


<p id="test" style="display: none">You've submitted a word</p>
<div class="contain-content">
    <form id="clueForm" name="clueForm" action="">
        <div class="clue-container">
            <div class="box"><input type="text" class="letter" name="clueLetter" /></div>
            <div class="box"><input type="text" class="letter" name="clueLetter" /></div>
            <div class="box"><input type="text" class="letter" name="clueLetter" /></div>
            <div class="box"><input type="text" class="letter" name="clueLetter" /></div>
            <div class="box"><input type="text" class="letter" name="clueLetter" /></div>
        </div>
        <button type="button" class="btn btn-default" id="addBox">+</button>
        <button type="button" class="btn btn-default" id="removeBox">-</button>
        <button type="button" class="btn btn-default" id="restart">Restart</button>
        <button type="button" class="btn" id="solveClue">Solve</button>

    </form>
    <div id="errorMsg" style="display:none">Errors</div>
    <div id="results" style="display: none">
        <h6>Solutions</h6>
        <ul id="solutions-list"></ul>
    </div>
</div>

<script type="text/javascript" src="js/solver.js"></script>

</body>
</html>
